<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog  
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"  
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"  
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd  http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.3.xsd">  
	
	<changeSet id="01.01" author="jogasingh" context="tst">
		<loadUpdateData  
	            encoding="UTF-8"  
	            file="customers.csv"  
	            primaryKey="id"  
	            quotchar="'"  
	            relativeToChangelogFile="true"    
	            separator=";"  
	            tableName="CUST">  
	        <column  header="id" name="id" type="STRING"/>  
	        <column  header="firstName" name="FIRST_NAME" type="STRING"/>   
	        <column  header="lastName" name="LAST_NAME" type="STRING"/>   
	        <column  header="email" name="EMAIL" type="STRING"/>   
	        <column  header="mobile" name="MOBILE" type="STRING"/>   
	        <column  header="phone" name="PHONE" type="STRING"/>
	    </loadUpdateData> 
    </changeSet>
	<changeSet id="01.02" author="jogasingh" context="tst">
		<loadUpdateData  
	            encoding="UTF-8"  
	            file="dhabas.csv"  
	            primaryKey="id"  
	            quotchar="'"  
	            relativeToChangelogFile="true"    
	            separator=";"  
	            tableName="DHABA">  
	        <column  header="id" name="id" type="STRING"/>  
	        <column  header="name" name="NAME" type="STRING"/>   
	        <column  header="taxno" name="TAX_NO" type="STRING"/>   
	        <column  header="email" name="ORDER_EMAIL" type="STRING"/>   
	        <column  header="phone" name="ORDER_PHONE" type="STRING"/>   
	        <column  header="self" name="DELIVERY_SELF" type="BOOLEAN"/>
	        <column  header="pickup" name="DELIVERY_PICKUP" type="BOOLEAN"/>
	        <column  header="dinein" name="DELIVERY_DINEIN" type="BOOLEAN"/>
	        <column  header="3rdparty" name="DELIVERY_3RDPARTY" type="BOOLEAN"/>
	    </loadUpdateData> 
    </changeSet>
   	<changeSet id="01.03" author="jogasingh" context="tst">
		<loadUpdateData  
	            encoding="UTF-8"  
	            file="addresses.csv"  
	            primaryKey="id"  
	            quotchar="'"  
	            relativeToChangelogFile="true"    
	            separator=";"  
	            tableName="ADDR">  
	        <column  header="id" name="id" type="STRING"/>  
	        <column  header="objtype" name="OBJ_TYPE" type="STRING"/>  
	        <column  header="objid" name="OBJ_ID" type="STRING"/>  
	        <column  header="street" name="STREET" type="STRING"/>   
	        <column  header="hno" name="HNO" type="STRING"/>   
	        <column  header="city" name="CITY" type="STRING"/>   
	        <column  header="area" name="AREA" type="STRING"/>   
	        <column  header="zipCode" name="ZIP" type="STRING"/>   
	        <column  header="state" name="STATE" type="STRING"/>
	        <column  header="countryId" name="COUNTRY_CODE" type="STRING"/>
	        <column  header="isdefault" name="IS_DEFAULT" type="BOOLEAN"/>   
	    </loadUpdateData> 
    </changeSet>

    <changeSet id="01.04" author="jogasingh" context="tst">
		<loadUpdateData  
	            encoding="UTF-8"  
	            file="system-prop.csv"  
	            primaryKey="name"  
	            quotchar="'"  
	            relativeToChangelogFile="true"    
	            separator=";"  
	            tableName="SYSTEM_PROP">  
	        <column  header="name" name="NAME" type="STRING"/>  
	        <column  header="value" name="VALUE" type="STRING"/>        
	    </loadUpdateData> 
    </changeSet>
</databaseChangeLog>